#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/cakra/Documents/app/vyral/node_modules/.pnpm/@turbo+workspaces@1.13.4/node_modules/@turbo/workspaces/dist/node_modules:/Users/cakra/Documents/app/vyral/node_modules/.pnpm/@turbo+workspaces@1.13.4/node_modules/@turbo/workspaces/node_modules:/Users/cakra/Documents/app/vyral/node_modules/.pnpm/@turbo+workspaces@1.13.4/node_modules/@turbo/node_modules:/Users/cakra/Documents/app/vyral/node_modules/.pnpm/@turbo+workspaces@1.13.4/node_modules:/Users/cakra/Documents/app/vyral/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/cakra/Documents/app/vyral/node_modules/.pnpm/@turbo+workspaces@1.13.4/node_modules/@turbo/workspaces/dist/node_modules:/Users/cakra/Documents/app/vyral/node_modules/.pnpm/@turbo+workspaces@1.13.4/node_modules/@turbo/workspaces/node_modules:/Users/cakra/Documents/app/vyral/node_modules/.pnpm/@turbo+workspaces@1.13.4/node_modules/@turbo/node_modules:/Users/cakra/Documents/app/vyral/node_modules/.pnpm/@turbo+workspaces@1.13.4/node_modules:/Users/cakra/Documents/app/vyral/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@turbo/workspaces/dist/cli.js" "$@"
else
  exec node  "$basedir/../@turbo/workspaces/dist/cli.js" "$@"
fi
